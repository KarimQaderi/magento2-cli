{
    "name": "m2/cli",
    "version": "1.2",
    "description": "Magento 2 Cli code",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "m2/cli-core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "M2\\Cli\\App\\": "app"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "M2\\Cli\\App\\ComposerScripts::postAutoloadDump"
        ]
    }
}
