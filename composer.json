{
    "name": "m2/cli",
    "version": "dev-master",
    "description": "Magento 2 Cli code",
    "type": "cli-mage-tool",
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "M2\\Cli\\": "src"
        }
    },
    "bin": [
        "bin/m2"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "M2\\Cli\\App\\ComposerScripts::postAutoloadDump"
        ]
    }
}
